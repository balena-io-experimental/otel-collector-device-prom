              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "(E|e)merg|(F|f)atal|(A|a)lert|(C|c)rit.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "purple",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "(E|e)(rr.*|RR.*)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "(W|w)(arn.*|ARN.*|rn|RN)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "(N|n)(otice|ote)|(I|i)(nf.*|NF.*)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "dbg.*|DBG.*|(D|d)(EBUG|ebug)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "(T|t)(race|RACE)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "light-blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "unknown"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "text",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "interval": "30s",
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "pluginVersion": "11.4.0-78029",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "$loki_datasource"
          },
          "editorMode": "code",
          "expr": "sum by (service_name) (count_over_time({service_name=\"unknown_service\",hostname=\"$instance\"} [$__interval]))\n",
          "legendFormat": "{{ level }}",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Logs volume",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "Value",
            "renamePattern": "unknown"
          }
        }
      ],
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "datasource",
        "uid": "-- Mixed --"
      },
      "gridPos": {
        "h": 18,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "id": 2,
      "options": {
        "dedupStrategy": "exact",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "pluginVersion": "11.4.0-78029",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "$loki_datasource"
          },
          "editorMode": "code",
          "expr": "{service_name=\"unknown_service\", hostname=\"$instance\"}",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Logs",
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "format": "json",
            "jsonPaths": [
              {
                "alias": "body_message",
                "path": "body.MESSAGE"
              },
              {
                "alias": "body_container",
                "path": "body.CONTAINER_NAME"
              }
            ],
            "keepTime": true,
            "replace": true,
            "source": "Line"
          }
        }
      ],
      "type": "logs"
    }
  ],
